{
  "name": "rankit",
  "version": "7.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "npx ng",
    "start": "node dist-server/main.js",
    "ssr": "npx ng run rankit:serve-ssr",
    "ssr:watch": "npx ng run rankit:serve-ssr:dev",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\"",
    "build": "npx ng build",
    "build:server": "npx ng run rankit:server:production",
    "build:prod": "npx ng build --prod",
    "build:universal": "npx ng run rankit:build:production && npx ng run rankit:server:production",
    "server": "node dist-server/main.js",
    "test": "npx ng test",
    "test:ci": "cross-env CI=true npx ng test",
    "test:ssr": "run-p test:ssr:*",
    "test:ssr:server": "cross-env TEST=true node dist-server/main.js",
    "test:ssr:script": "node ./node_modules/npm-delay 2000 && ts-mocha test/*.spec.ts test/exit.ts --timeout 15000",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "bundle-report": "npx ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "bundle-report:prod": "npx ng build --stats-json --prod && webpack-bundle-analyzer dist/stats.json",
    "dev:ssr": "npx ng run rankit:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "npx ng build --prod && npx ng run rankit:server:production",
    "prerender": "npx ng run rankit:prerender",
    "postinstall1": "ngcc",
    "deploy": "npx ng build --prod && npm run build:ssr && gcloud app deploy",
    "staging:deploy": "npx ng build --prod && npm run build:ssr && gcloud app deploy --no-promote -q"
  },
  "private": true,
  "dependencies": {
    "@angular-material-extensions/password-strength": "^6.0.0",
    "@angular/animations": "9.0.7",
    "@angular/cdk": "^9.0.7",
    "@angular/common": "9.0.7",
    "@angular/compiler": "9.0.7",
    "@angular/core": "9.0.7",
    "@angular/fire": "^6.0.3",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "9.0.7",
    "@angular/material": "^9.0.7",
    "@angular/platform-browser": "9.0.7",
    "@angular/platform-browser-dynamic": "9.0.7",
    "@angular/router": "9.0.7",
    "@angular/service-worker": "9.0.7",
    "@gorniv/ngx-universal": "2.2.2",
    "@nguniversal/common": "9.0.2",
    "@nguniversal/express-engine": "9.0.2",
    "@ngx-translate/core": "12.1.2",
    "@ngx-translate/http-loader": "4.0.0",
    "@rxweb/reactive-form-validators": "^1.9.9",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.4",
    "express": "4.17.1",
    "firebase": "^7.14.3",
    "firebaseui": "^4.5.0",
    "firebaseui-angular": "^4.1.0",
    "ngx-auth-firebaseui": "^4.2.1",
    "ngx-cookie-service": "^3.0.4",
    "ngx-meta": "^0.0.1",
    "node-cache": "5.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.4",
    "rxjs-compat": "^6.5.5",
    "rxjs-tslint": "^0.1.8",
    "tslib": "1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "9.0.7",
    "@angular/language-service": "9.0.7",
    "@angular/platform-server": "9.0.7",
    "@nguniversal/builders": "9.0.2",
    "@types/chai": "4.2.11",
    "@types/express": "4.17.3",
    "@types/jasmine": "3.5.9",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.1",
    "chai": "4.2.0",
    "codelyzer": "5.2.1",
    "cross-env": "5.2.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "^5.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "mocha": "^8.1.3",
    "nodemon": "1.19.4",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "prettier-tslint": "0.4.2",
    "protractor": "^7.0.0",
    "source-map-support": "0.5.16",
    "ssri": "6.0.1",
    "ts-mocha": "6.0.0",
    "ts-node": "8.8.0",
    "tslint": "5.20.1",
    "typescript": "3.7.5",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11",
    "webpack-node-externals": "1.7.2"
  }
}
